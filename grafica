#include <graphics.h>
#include <winbgim.h>
int main() {
    initwindow(1280,720);
    setbkcolor(WHITE);
    cleardevice();
    setfillstyle(SOLID_FILL, COLOR(47,61,76));
    bar(0, 0, 1280, 60);
    bar(0,660,1280,720);
    setfillstyle(SOLID_FILL, COLOR(151,159,165));
    bar(0,60,615,660);
    bar(640,60,1255,660);
    setfillstyle(SOLID_FILL, BLACK);
    bar(0, 30, 1280, 32);
    bar(0, 60, 1280, 64);
    bar(0, 690, 1280, 692);
    bar(0, 720, 1280, 724);
    bar(640, 0, 642, 690);
    bar(615, 60, 617, 690);
    bar(1255, 60, 1257, 690);
    bar(0, 90, 1280, 94);
    bar(0, 630, 1280, 632);
    bar(0,660,1280,662);

    bar(252, 690, 257, 720);//butoanele de jos (comenzile rapide)
    bar(509, 690, 514, 720);
    bar(766, 690, 771, 720);
    bar(1023, 690, 1028, 720);

    bar(270, 60, 273, 90);  //butoanele de deasupra fisierelor
    bar(355, 60, 358, 90);
    bar(440, 60, 443, 90);
    bar(910, 60, 913, 90);
    bar(995, 60, 998, 90);
    bar(1080, 60, 1083, 90);

    bar(271,90,272,630); //liniile ce delimiteaza informatiile
    bar(356, 90, 357, 630);
    bar(441, 90, 442, 630);
    bar(911, 90, 912, 630);
    bar(996, 90, 997, 630);
    bar(1081, 90, 1082, 630);


    getch();
    closegraph();

    return 0;
}
